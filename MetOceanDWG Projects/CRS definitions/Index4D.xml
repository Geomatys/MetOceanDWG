<?xml version="1.0" encoding="UTF-8"?>
<ImageCRS xmlns="http://www.opengis.net/gml/3.2"
          xmlns:xlink="http://www.w3.org/1999/xlink"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns:gml="http://www.opengis.net/gml/3.2"
          xmlns:gmd="http://www.isotc211.org/2005/gmd"
          xmlns:gco="http://www.isotc211.org/2005/gco"
          xmlns:rim="urn:oasis:names:tc:ebxml-regrep:xsd:rim:3.0"
          xmlns:epsg="urn:x-ogp:spec:schema-xsd:EPSG:1.0:dataset"
          xsi:schemaLocation = "http://www.opengis.net/gml/3.2 http://schemas.opengis.net/gml/3.2.1/datums.xsd"
          gml:id="index4d-crs">
  <description>CRS for 4D index-domain referenced axis.</description>
  <identifier codeSpace="http://www.ietf.org/rfc/rfc3986">http://www.opengis.net/def/crs/OGC/0.2/Index4D</identifier>
  <name>4D Index CRS</name>
  <remarks>Version 0.2</remarks>
  <scope>not known</scope>
  <cartesianCS>
    <CartesianCS gml:id="index4d-crs_cartesian4D">
      <description/>
      <identifier codeSpace="http://www.ietf.org/rfc/rfc3986">%cartesianIndexed4D%</identifier>
      <remarks>Version 0.2</remarks>
      <axis>
        <CoordinateSystemAxis gml:id="index4d-crs_cartesian4D_index-cs_4D_1" uom="http://www.opengis.net/def/uom/OGC/1.0/GridSpacing">
          <description>1D coordinate system with indexed coordinates [GridSpacing].</description>
          <identifier codeSpace="http://www.ietf.org/rfc/rfc3986">%index-cs%</identifier>
          <remarks>Version 0.2</remarks>
          <axisAbbrev>i</axisAbbrev>
          <axisDirection codeSpace="http://www.ietf.org/rfc/rfc3986">http://www.opengis.net/def/axisDirection/OGC/0.2/indexedAxisPositive</axisDirection>
        </CoordinateSystemAxis>
      </axis>
      <axis>
        <CoordinateSystemAxis gml:id="index4d-crs_cartesian4D_index-cs_4D_2" uom="http://www.opengis.net/def/uom/OGC/1.0/GridSpacing">
          <description>1D coordinate system with indexed coordinates [GridSpacing].</description>
          <identifier codeSpace="http://www.ietf.org/rfc/rfc3986">%index-cs%</identifier>
          <remarks>Version 0.2</remarks>
          <axisAbbrev>j</axisAbbrev>
          <axisDirection codeSpace="http://www.ietf.org/rfc/rfc3986">http://www.opengis.net/def/axisDirection/OGC/0.2/indexedAxisPositive</axisDirection>
        </CoordinateSystemAxis>
      </axis>
      <axis>
        <CoordinateSystemAxis gml:id="index4d-crs_cartesian4D_index-cs_4D_3" uom="http://www.opengis.net/def/uom/OGC/1.0/GridSpacing">
          <description>1D coordinate system with indexed coordinates [GridSpacing].</description>
          <identifier codeSpace="http://www.ietf.org/rfc/rfc3986">%index-cs%</identifier>
          <remarks>Version 0.2</remarks>
          <axisAbbrev>k</axisAbbrev>
          <axisDirection codeSpace="http://www.ietf.org/rfc/rfc3986">http://www.opengis.net/def/axisDirection/OGC/0.2/indexedAxisPositive</axisDirection>
        </CoordinateSystemAxis>
      </axis>
      <axis>
        <CoordinateSystemAxis gml:id="index4d-crs_cartesian4D_index-cs_4D_4" uom="http://www.opengis.net/def/uom/OGC/1.0/GridSpacing">
          <description>1D coordinate system with indexed coordinates [GridSpacing].</description>
          <identifier codeSpace="http://www.ietf.org/rfc/rfc3986">%index-cs%</identifier>
          <remarks>Version 0.2</remarks>
          <axisAbbrev>m</axisAbbrev>
          <axisDirection codeSpace="http://www.ietf.org/rfc/rfc3986">http://www.opengis.net/def/axisDirection/OGC/0.2/indexedAxisPositive</axisDirection>
        </CoordinateSystemAxis>
      </axis>
    </CartesianCS>
  </cartesianCS>
  <imageDatum>
    <ImageDatum gml:id="index4d-crs_indexnd-td-corner">
      <description>
        A general nD image datum with the origin at the lower bound of the origin cell in the axis.
      </description>
      <identifier codeSpace="http://www.ietf.org/rfc/rfc3986">%indexnd-td-corner%</identifier>
      <remarks>Version 0.2</remarks>
      <scope>not known</scope>
      <pixelInCell codeSpace="http://www.ietf.org/rfc/rfc3986">http://www.opengis.net/def/pixelInCell/OGC/1.0/cellCenter</pixelInCell>
    </ImageDatum>
  </imageDatum>
</ImageCRS>
